{% extends 'base.html.twig' %}

{% block title %}Détail du module{% endblock %}

{% block body %}

<div class="wrapper">
    <h1>Détail du module</h1>
    <article class="info-module">

        <h2>{{ module }}</h2>

        <p>{{ module.description }}</p>

    </article>
    <p>Vous souhaitez modifier le titre ou la description ? </p>
    <a href="{{ path('edit_module', {'id':module.id}) }}">Editer</a>
</div>
{% endblock %}